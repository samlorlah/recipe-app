<% if user_signed_in? %>
  <%= button_to 'Sign out', destroy_user_session_path(@user), method: :delete, data: { turbo: false }, class: "" %>
  <% @users.each do |user| %>
    <%= link_to user_path(user), class: "" do %>
        <h1 class=""><%= user.name %></h1>
    <% end %>
  <% end %>
<% else %>
  <div class=""><%= link_to 'Sign In', new_user_session_path, class: "" %></div>
<% end %>
